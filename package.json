    "apollo-server": "^2.8.1",
    "colors": "^1.3.3",
    "concurrently": "^4.1.1",
    "graphql": "^14.4.2",
    "graphql-import-node": "^0.0.4",
    "ts-node": "^8.3.0",
    "ts-node-dev": "^1.0.0-pre.40",
    "typescript": "^3.5.3"
  },
  "devDependencies": {
    "@types/graphql": "^14.2.3"
  }
}
